{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2021",
    "lib": [
      "es2021",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "tool",
    "eslint.config.mts"
  ]
}